/*
= Hides input quantity
= Creates select quantity
= Calculate pricePerItem
Output Total

================================================ */
jQuery(document).ready(function(){
  jQuery('form.product-form .input-wrapper input#quantity').attr('max', 10000);
  jQuery('form.product-form .input-wrapper input#quantity').hide();
  jQuery('form.product-form .input-wrapper input#quantity').after('<select class="select-quantity-by-hundred"></select>');

  var selectQuantityByHundred_Options = 500;
  while (selectQuantityByHundred_Options <= 10000){
    if (selectQuantityByHundred_Options == 500 ){
      jQuery('form.product-form .select-quantity-by-hundred').append('<option selected value="'+selectQuantityByHundred_Options+'">'+selectQuantityByHundred_Options+'</option>');
    } else {
      jQuery('form.product-form .select-quantity-by-hundred').append('<option value="'+selectQuantityByHundred_Options+'">'+selectQuantityByHundred_Options+'</option>');
    }
    selectQuantityByHundred_Options = selectQuantityByHundred_Options + 100;
  }
});
jQuery(document).on('change','select.select-quantity-by-hundred',function(){

  /* Pass Form Item Quantity through put numberOfItems in input quantity */  
  var numberOfItems = jQuery(this).val();
  jQuery('form.product-form .input-wrapper input#quantity').val(numberOfItems);
  console.log(numberOfItems);

  /* Calc Discount */
  var pricePerItem = 12.50;
  if (numberOfItems >= 1000){
    pricePerItem = 10.50;
  } else {
    switch(parseInt(numberOfItems)) {
      case 500:
          pricePerItem = 12.50;
          break;
      case 600:
          pricePerItem = 12.10;
          break;
      case 700:
          pricePerItem = 11.70;
        break;
      case 800:
          pricePerItem = 11.30;
        break;
      case 900:
          pricePerItem = 10.90;
        break;
      case 1000:
          pricePerItem = 10.50;
        break;
    }
  } console.log('Price per item: '+pricePerItem);
  jQuery('form.product-form .input-wrapper input#countbasicamount').val('$ '+pricePerItem+'0');

  /* Calc Total */
  var total;
  total = Math.ceil(numberOfItems * pricePerItem);
  jQuery('form .product-price #showTotal').text(total);
  /* test */
  console.log('Value of Changed Item '+numberOfItems);

});
