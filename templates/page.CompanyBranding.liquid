   <div id="enterQuantity">Enter Quantity</div>
      <div class="input-wrapper product-quantity">
        <label for="quantity">{{ 'general.quantity' | t }}</label>
        <input type="number" name="quantity" id="quantity" value="500"  min="500" max="3000" step='100' maximum='20'/>
      </div>
  <div class="product-price input-wrapper" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
        <span class="price" itemprop="price"> 
           <label style="position:relative;top:-4.5px">Price Per:</label>
          <input type='text' readonly='readonly' class="money" id="countbasicamount" value='{{ selectedVariant.price | money }}' style="margin-top:-6px"> 
       
          {% unless shappify_qb_options_count == 1 and bold_compare_at_price_min <= selectedVariant.price %}
            <span class="original money" style="display:inline;">
              {% if selectedVariant.compare_at_price > selectedVariant.price %}
                {{ selectedVariant.compare_at_price | money }}
              {% endif %}
            </span>
          {% endunless %}
        </span>
    
      <p  style="display:inline;font-size:16px"> Total: $</p>
      <div id="showTotal"  style="display:inline;font-size:16px">
        
      
      </div><br/><br />
      </div>
      <script type="text/javascript">
          $('#quantity').keyup(function(){
          var money = $('#countbasicamount').val();
          money = money.replace('$','');
           
          money = money.replace(' ','');
          money = parseFloat(money);
          var showTotal = $('#quantity').val()*money;
        $('#showTotal').html(showTotal);
        });
      </script>
      <div class="add-to-cart">
        
        
        {% if product.available %}
        <input type="submit" value="NEXT" />
          <div class="product-form-submit-wrap">
